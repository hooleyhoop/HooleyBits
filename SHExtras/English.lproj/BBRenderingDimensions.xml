<dict>
	
	<key>nodeAttributes</key>
	<dict>
		<key>copyright</key>
		<string>Best Before Media Ltd. 2006</string>
		<key>category</key>
		<string>Goatse</string>
		<key>name</key>
		<string>BB Rendering Destination Dimensions</string>
